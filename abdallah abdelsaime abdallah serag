def selection_sort(arr):
    n = len(arr)
    for i in range(n):
        min_idx = i
        for j in range(i + 1, n):   
            if arr[j] < arr[min_idx]:
                min_idx = j  
        arr[i], arr[min_idx] = arr[min_idx], arr[i] 
    return arr

 
arr = [20, 35, 5, 80, 33, 120, 40]
print( "sort is ::" , selection_sort(arr))
